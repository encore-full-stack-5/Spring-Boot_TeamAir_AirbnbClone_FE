v id=\"nowHosting\"\r\n                    @click=\"ActiveCategory('nowHosting')\">\r\n                    현재 호스팅 중({{ nowHosting }})건\r\n                </div>\r\n                <div id=\"checkIn\"\r\n                    @click=\"ActiveCategory('checkIn')\">\r\n                    체크인 예정({{ checkIn }})건\r\n                </div>\r\n                <div id=\"schedule\"\r\n                    @click=\"ActiveCategory('schedule')\">\r\n                    예정({{ schedule }})건\r\n                </div>\r\n                <div id=\"needReview\"\r\n                    @click=\"ActiveCategory('needReview')\">\r\n                    작성할 후기({{ needReview }})건\r\n                </div>\r\n            </div>\r\n            <div class=\"reserveListBox\">\r\n                \r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\n    import NavBar from '../../components/NavBar/HostNavBar.vue'\r\n\r\n    export default {\r\n        name: \"HostMainView\",\r\n        components: {\r\n            NavBar\r\n        },\r\n        data() {\r\n            return {\r\n                activeCategory: \"checkOut\",\r\n                hostName: \"{namehere}\",\r\n                totalReserve: 0,\r\n                checkOut: 0,\r\n                checkIn: 0,\r\n                nowHosting: 0,\r\n                schedule: 0,\r\n                needReview: 0\r\n            }\r\n        },\r\n        mounted() {\r\n            this.ActiveCategory(this.activeCategory)\r\n        },\r\n        methods: {\r\n            ActiveCategory(e) {\r\n                const beforeCategory = document.getElementById(this.activeCategory);\r\n                beforeCategory.style.setProperty('--bg-color', null);\r\n                beforeCategory.style.setProperty('--border', null);\r\n                this.activeCategory = e;\r\n                const category = document.getElementById(this.activeCategory);\r\n                category.style.setProperty('--bg-color', \"rgb(245, 245, 245)\");\r\n                category.style.setProperty('--border', \"2px solid black\");\r\n            },\r\n        },\r\n    }\r\n</script>\r\n<style>\r\n.hostMainBox{\r\n    max-width: 1300px;\r\n    margin: auto;\r\n    padding: 60px;\r\n    padding-top: 30px;\r\n    padding-bottom: 0px;\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n.reserveHead{\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: space-between;\r\n    margin-top: 20px;\r\n    margin-bottom: 10px;\r\n}\r\n.reserveCategory{\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: left;\r\n    gap: 8px;\r\n}\r\n.reserveCategory > div {\r\n    border: var(--border, 1px solid lightgray);\r\n    border-radius: 50vi;\r\n    background-color: var(--bg-color);\r\n    padding: 8px;\r\n    padding-left: 12px;\r\n    padding-right: 12px;\r\n    font-size: 10pt;\r\n    cursor: pointer;\r\n}\r\n.reserveCategory > div:hover {\r\n    border: var(--border, 1px solid black);\r\n}\r\n.reserveListBox {\r\n    width: 100%;\r\n    min-height: 200px;\r\n    border-radius: 10px;\r\n    background-color: rgb(245, 245, 245);\r\n    margin-top: 30px;\r\n}\r\n</style>"],"mappings":"AAsCI,OAAOA,MAAK,MAAO,wCAAuC;AAE1D,eAAe;EACXC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,cAAc,EAAE,UAAU;MAC1BC,QAAQ,EAAE,YAAY;MACtBC,YAAY,EAAE,CAAC;MACfC,QAAQ,EAAE,CAAC;MACXC,OAAO,EAAE,CAAC;MACVC,UAAU,EAAE,CAAC;MACbC,QAAQ,EAAE,CAAC;MACXC,UAAU,EAAE;IAChB;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,cAAc,CAAC,IAAI,CAACT,cAAc;EAC3C,CAAC;EACDU,OAAO,EAAE;IACLD,cAAcA,CAACE,CAAC,EAAE;MACd,MAAMC,cAAa,GAAIC,QAAQ,CAACC,cAAc,CAAC,IAAI,CAACd,cAAc,CAAC;MACnEY,cAAc,CAACG,KAAK,CAACC,WAAW,CAAC,YAAY,EAAE,IAAI,CAAC;MACpDJ,cAAc,CAACG,KAAK,CAACC,WAAW,CAAC,UAAU,EAAE,IAAI,CAAC;MAClD,IAAI,CAAChB,cAAa,GAAIW,CAAC;MACvB,MAAMM,QAAO,GAAIJ,QAAQ,CAACC,cAAc,CAAC,IAAI,CAACd,cAAc,CAAC;MAC7DiB,QAAQ,CAACF,KAAK,CAACC,WAAW,CAAC,YAAY,EAAE,oBAAoB,CAAC;MAC9DC,QAAQ,CAACF,KAAK,CAACC,WAAW,CA