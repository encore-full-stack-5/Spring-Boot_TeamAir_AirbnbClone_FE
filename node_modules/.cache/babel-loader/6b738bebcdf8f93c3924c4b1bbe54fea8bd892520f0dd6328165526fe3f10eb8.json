         if (this.markers.length > 0) {\r\n                this.markers.forEach(marker => {\r\n                    marker.setMap(null);\r\n                });\r\n                this.markers = [];\r\n            }\r\n            const LatLng = this.map.getCenter();\r\n            this.circle = new kakao.maps.Circle({\r\n                center : new kakao.maps.LatLng(LatLng.getLat(), LatLng.getLng()),\r\n                radius: 75,\r\n                strokeWeight: 0,\r\n                fillColor: '#ff0000',\r\n                fillOpacity: 0.5,\r\n            });\r\n            this.circle.setMap(this.map); \r\n        } else {\r\n            if (this.circle != null) {\r\n                this.circle.setMap(null);\r\n                this.circle = null;\r\n            }\r\n            const LatLng = this.map.getCenter();\r\n            this.displayMarker([LatLng.getLat(), LatLng.getLng()]);\r\n        }\r\n    }\r\n},\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n#map {\r\n    width: 10px;\r\n    height: 10px;\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n</style>"],"mappings":";;gEAEMA,mBAAA,CAAoB;EAAfC,EAAE,EAAC;AAAK;oBAAbC,UAAoB,C;;uBADtBC,mBAAA,CAEM,aAHV,IAAAC,UAAA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}